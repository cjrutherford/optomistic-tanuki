---
- nodejs-extensions:
  - .ts
  - .js
  ignore-rules:
  - pug_jade_templates
  ignore-extensions:
  - .test.ts
  - .spec.ts
  - .json
  - .entity.test.ts
  - .dto.test.ts
  ignore-paths:
  - __MACOSX
  - skip_dir
  - node_modules
  - dist
  - "**/*.spec.ts"
  - "**/*.test.ts"
  severity-filter:
  - WARNING
  - ERROR
